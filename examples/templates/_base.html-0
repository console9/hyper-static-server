<!DOCTYPE html>

<html lang="en">
	
	
	<head>
		
		
		<title>{% block page_title %}{% endblock %}</title>
		
		<meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, minimum-scale=0.5, maximum-scale=1.0, user-scalable=yes" />
		
		{% block head %}{% endblock %}
		
		{% if __is_development %}
		<script src="/__/reload.js" defer></script>
		{% endif %}
		
	</head>
	
	
	<body class="{% block page_classes %}{% endblock %}">
		
		<header>
			{% block header %}{% include "_header.html-0" %}{% endblock %}
		</header>
		
		<main>
			{% block main %}<h1>TBD</h1>{% endblock %}
		</main>
		
		<footer>
			{% block footer %}{% include "_footer.html-0" %}{% endblock %}
		</footer>
		
	</body>
	
	
</html>

